{
  "cli": {
    "version": ">= 0.60.0",
    "promptToConfigurePushNotifications": true
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "distribution": "internal"
      },
      "env": { "EXPO_PUBLIC_API_URL": "https://turbojet.originalcake-hk.com" }
    },
    "dev_client": {
      "developmentClient": true,
      "android": {
        "buildType": "apk",
        "distribution": "internal"
      },
      "env": {
        "ANDROID_SDK_ROOT": "~/android",
        "EXPO_PUBLIC_API_URL": "https://turbojet.originalcake-hk.com"
      },
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "env": {
        "ANDROID_SDK_ROOT": "~/android",
        "EXPO_PUBLIC_API_URL": "https://turbojet.originalcake-hk.com"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
